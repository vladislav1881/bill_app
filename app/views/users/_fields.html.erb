<%= render 'shared/error_messages', object: f.object %>

<%= f.label t(:name) %>
<%= f.text_field :name %>

<%= f.label t(:email) %>
<%= f.text_field :email %>

<%= f.label t(:clubs) %>
<%= f.collection_select :club_ids, Club.order(:name), :id, :name, {  include_blank: true }, size: 3, multiple: true %>

<%= f.label t(:password) %>
<%= f.password_field :password %>

<%= f.label :password_confirmation, t(:pass_conf) %>
<%= f.password_field :password_confirmation %>

